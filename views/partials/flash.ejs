<% if (success && success.length) { %>
    <div class="container">
        <div class="alert alert-success alert-dismissible fade show col-md-6 offset-md-3" role="alert">
            <%= success %> 
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    </div>
<% } %>

